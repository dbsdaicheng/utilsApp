{
  "name": "utilsApp",
  "version": "1.0.0",
  "private": true,
  "description": "工具集小程序",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "cross-env NODE_ENV=prod taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "cross-env NODE_ENV=dev taro build --type weapp --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test:weapp": "cross-env NODE_ENV=dev taro build --type weapp --watch",
    "test:swan": "npm run build:swan -- --watch",
    "test:alipay": "npm run build:alipay -- --watch",
    "test:tt": "npm run build:tt -- --watch",
    "test:h5": "npm run build:h5 -- --watch",
    "test:rn": "npm run build:rn -- --watch",
    "test:qq": "npm run build:qq -- --watch",
    "test:jd": "npm run build:jd -- --watch",
    "test:quickapp": "npm run build:quickapp -- --watch "
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "^3.4.9",
    "@tarojs/components": "3.4.3",
    "@tarojs/plugin-framework-react": "3.4.3",
    "@tarojs/react": "3.4.3",
    "@tarojs/runtime": "3.4.3",
    "@tarojs/taro": "3.4.3",
    "cross-env": "^7.0.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "taro-hooks": "^1.5.8",
    "taro-ui": "^3.0.0-alpha"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.4.3",
    "@tarojs/webpack-runner": "3.4.3",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.4.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "stylelint": "^14.4.0"
  }
}
